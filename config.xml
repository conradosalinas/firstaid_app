<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
        versionCode = "10"
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>FirstAid</name>

    <description>
        An example for phonegap build docs.
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        Hardeep Shoker
    </author>

    <!-- HealthKit Plugin -->
    <gap:plugin name="com.telerik.plugins.healthkit" version="0.1.0" />

    <!-- Geolocation Plugin -->
    <gap:plugin name="fi.avaus.cordova.geolocation" version="0.3.11" />

    <!-- GoogleNavigate Plugin -->
    <gap:plugin name="dk.interface.cordova.plugin.googlenavigate" version="0.1.2" />

</widget>